import java.util.*;
public class Program
{
    public static void main(String[] args) {
		int a[]={2, 1, -2, 3 ,4, 3, 2, 1};
    
        int n=a.length;
        HashSet<Integer>hm=new HashSet<>();
        for(int i=0;i<n;i++)
        {
            if(!hm.contains(a[i]))
            hm.add(a[i]);
        }
          System.out.print(hm);
    }
  
}
